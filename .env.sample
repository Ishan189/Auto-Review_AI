# =========================================
# Rate Limiting Configuration
# =========================================
# Adjust these to control how fast the script runs
# Higher values = Safer but slower
# Lower values = Faster but risky

# Delay between individual requests (in seconds)
MIN_DELAY_BETWEEN_REQUESTS=2
MAX_DELAY_BETWEEN_REQUESTS=3

# Delay between batches (in seconds)
MIN_DELAY_BETWEEN_BATCHES=30
MAX_DELAY_BETWEEN_BATCHES=60

# Processing Configuration
BATCH_SIZE=5           # Number of submissions per batch
RETRY_BASE_DELAY=10             # Base delay for retries (will multiply by attempt number)
MAX_RETRIES=3                   # Maximum retry attempts

GEMINI_API_KEY=''

# =========================================
# Tips to Avoid Rate Limiting:
# =========================================
# - If you get blocked often: INCREASE all delay values
# - For overnight processing: Set MIN=10, MAX=20, BATCHES=30-60
# - For faster (risky): Set MIN=1, MAX=3, but not recommended
# - See CONFIG.md for detailed guide
